<UserControl
    x:Class="SiPintar.Views.Hublang.Pelayanan.Pendaftaran.KoreksiFormView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:SiPintar.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:gmaps="clr-namespace:GMap.NET.WindowsPresentation;assembly=GMap.NET.WindowsPresentation"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:other="clr-namespace:SiPintar.Views.Global.Other"
    mc:Ignorable="d">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/SiPintar;component/Assets/ResourceDictionaries/SideNavigationStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <converters:BooleanToVisibilityConverter
                x:Key="BooleanToVisibleConverter"
                False="Collapsed"
                True="Visible" />
            <converters:BooleanToVisibilityConverter
                x:Key="BooleanToCollapseConverter"
                False="Visible"
                True="Collapsed" />
            <converters:InverseBooleanConverter x:Key="InverseBooleanConverter" />
            <converters:StringDefaultValueConverter x:Key="StringDefaultValueConverter" Default="-" />
            <converters:NullVisibilityConverter
                x:Key="NullToCollapseConverter"
                False="Visible"
                True="Collapsed" />
            <converters:NullVisibilityConverter
                x:Key="NullToVisibleConverter"
                False="Collapsed"
                True="Visible" />
            <converters:DatetimeToStringConverter x:Key="DatetimeToStringConverter" />

            <Style x:Key="IconZoom" TargetType="TextBlock">
                <Setter Property="FontFamily" Value="{StaticResource MontserratMedium}" />
                <Setter Property="Foreground" Value="{StaticResource Astronaut}" />
                <Setter Property="FontSize" Value="24px" />
                <Setter Property="HorizontalAlignment" Value="Center" />
                <Setter Property="Cursor" Value="Hand" />
            </Style>
        </ResourceDictionary>
    </UserControl.Resources>
    <md:DialogHost DialogTheme="Inherit" Identifier="InnerHublangRootDialog">
        <Grid>
            <Border
                Width="1000"
                MaxHeight="700"
                Background="White">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>

                    <StackPanel
                        Grid.Row="0"
                        Background="{StaticResource BaseAppHublang}"
                        Orientation="Vertical">
                        <Grid Margin="10,5">
                            <Grid Margin="10,7,15,5">
                                <TextBlock
                                    x:Name="Title"
                                    Foreground="White"
                                    LineHeight="10"
                                    Style="{StaticResource BigTitle}"
                                    Text="Koreksi Pendaftaran Sambungan Baru" />
                            </Grid>
                            <StackPanel
                                HorizontalAlignment="Right"
                                VerticalAlignment="Top"
                                Orientation="Horizontal">
                                <Border
                                    Height="16"
                                    Margin="5,0"
                                    Background="White"
                                    CornerRadius="2">
                                    <TextBlock
                                        Padding="6,1,6,1"
                                        FontSize="10"
                                        FontWeight="SemiBold"
                                        Foreground="{StaticResource BaseAppHublang}">
                                        Esc
                                    </TextBlock>
                                </Border>

                                <Button
                                    Width="14"
                                    Height="14"
                                    Margin="8"
                                    Padding="0"
                                    Background="{x:Null}"
                                    Command="{x:Static md:DialogHost.CloseDialogCommand}"
                                    Style="{StaticResource ButtonPlain}">
                                    <md:PackIcon
                                        Margin="0"
                                        Foreground="White"
                                        Kind="Close" />
                                </Button>
                            </StackPanel>

                        </Grid>
                    </StackPanel>
                    <Grid Grid.Row="1" Margin="16,16,16,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition />
                        </Grid.RowDefinitions>

                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <StackPanel Margin="0,0,20,0">
                                <StackPanel Margin="0,0,0,0" Orientation="Horizontal">
                                    <RadioButton
                                        x:Name="DataPendaftaran"
                                        Checked="SubMenu_Checked"
                                        Content="Data Pendaftaran"
                                        FontSize="13"
                                        FontWeight="DemiBold"
                                        GroupName="SubMenu"
                                        Style="{StaticResource NavHorizontalStyle}" />
                                    <RadioButton
                                        x:Name="SuratKelengkapan"
                                        Checked="SubMenu_Checked"
                                        Content="Surat Kelengkapan"
                                        FontSize="13"
                                        FontWeight="DemiBold"
                                        GroupName="SubMenu"
                                        Style="{StaticResource NavHorizontalStyle}" />
                                </StackPanel>

                                <Border
                                    Height="1.2"
                                    Margin="0,0,0,-1"
                                    VerticalAlignment="Bottom"
                                    Background="{StaticResource BaseGray}" />
                            </StackPanel>

                            <Grid
                                Grid.Column="1"
                                Margin="0,0,0,15"
                                HorizontalAlignment="Right">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="150" />
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="310" />
                                </Grid.ColumnDefinitions>

                                <Border
                                    Grid.Column="0"
                                    Padding="10,5,10,5"
                                    Background="{StaticResource Black10}">
                                    <StackPanel>
                                        <TextBlock
                                            Margin="0,0,0,5"
                                            FontFamily="{StaticResource SarabunRegular}"
                                            FontSize="12"
                                            Foreground="#646464"
                                            Text="Nomor Pendaftaran" />
                                        <TextBlock
                                            FontFamily="{StaticResource MontserratSemiBold}"
                                            FontSize="18"
                                            Foreground="{Binding SelectedData.NomorPermohonan, Converter={StaticResource StringDefaultValueConverter}}"
                                            Text="{Binding SelectedData.NomorPermohonan, Converter={StaticResource StringDefaultValueConverter}}" />
                                    </StackPanel>
                                </Border>

                                <Border
                                    x:Name="StatusBg"
                                    Grid.Column="2"
                                    Padding="10,5,10,5"
                                    Background="{StaticResource Black10}">
                                    <StackPanel>
                                        <TextBlock
                                            Margin="0,0,0,5"
                                            FontFamily="{StaticResource SarabunRegular}"
                                            FontSize="12"
                                            Foreground="#646464"
                                            Text="Status" />
                                        <TextBlock
                                            x:Name="StatusText"
                                            FontFamily="{StaticResource MontserratSemiBold}"
                                            FontSize="18"
                                            Foreground="{Binding SelectedData.StatusPermohonanText, Converter={StaticResource StringDefaultValueConverter}}"
                                            Text="{Binding SelectedData.StatusPermohonanText, Converter={StaticResource StringDefaultValueConverter}}" />
                                    </StackPanel>
                                </Border>

                            </Grid>
                        </Grid>

                        <Grid
                            x:Name="DataPendaftaranSection"
                            Grid.Row="1"
                            Panel.ZIndex="2"
                            Background="White">
                            <ScrollViewer Margin="0,0,-16,0" VerticalScrollBarVisibility="Auto">
                                <StackPanel Margin="20">
                                    <Grid
                                        x:Name="StepInfo"
                                        Margin="0,0,0,30"
                                        Visibility="{Binding IsEdit, Converter={StaticResource BooleanToCollapseConverter}}">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid
                                            Grid.Column="0"
                                            Margin="0,15,0,0"
                                            HorizontalAlignment="Center">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="28" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="28" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="28" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="28" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="28" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="28" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="28" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>

                                            <StackPanel
                                                Grid.Column="0"
                                                HorizontalAlignment="Right"
                                                Orientation="Horizontal">
                                                <Border
                                                    x:Name="StepNumber1"
                                                    Width="20"
                                                    Height="20"
                                                    Background="#0088E2"
                                                    CornerRadius="50">
                                                    <TextBlock
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        FontFamily="{StaticResource SarabunRegular}"
                                                        FontSize="11"
                                                        Foreground="White"
                                                        Text="1" />
                                                </Border>
                                                <TextBlock
                                                    x:Name="StepText1"
                                                    Margin="10,0,0,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="{StaticResource SarabunRegular}"
                                                    FontSize="11"
                                                    FontWeight="SemiBold"
                                                    Foreground="#252B46"
                                                    Text="Permohonan" />
                                            </StackPanel>
                                            <StackPanel Grid.Column="1" VerticalAlignment="Center">
                                                <Border
                                                    Width="15"
                                                    Height="1"
                                                    Background="#A7A7A7" />
                                            </StackPanel>
                                            <StackPanel
                                                Grid.Column="2"
                                                HorizontalAlignment="Center"
                                                Orientation="Horizontal">
                                                <Border
                                                    x:Name="StepNumber2"
                                                    Width="20"
                                                    Height="20"
                                                    Background="#BDBDBD"
                                                    CornerRadius="50">
                                                    <TextBlock
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        FontFamily="{StaticResource SarabunRegular}"
                                                        FontSize="11"
                                                        Foreground="White"
                                                        Text="2" />
                                                </Border>
                                                <TextBlock
                                                    x:Name="StepText2"
                                                    Margin="10,0,0,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="{StaticResource SarabunRegular}"
                                                    FontSize="11"
                                                    FontWeight="SemiBold"
                                                    Foreground="#A7A7A7"
                                                    Text="Bayar Reguler" />
                                            </StackPanel>
                                            <StackPanel Grid.Column="3" VerticalAlignment="Center">
                                                <Border
                                                    Width="15"
                                                    Height="1"
                                                    Background="#A7A7A7" />
                                            </StackPanel>
                                            <StackPanel
                                                Grid.Column="4"
                                                HorizontalAlignment="Left"
                                                Orientation="Horizontal">
                                                <Border
                                                    x:Name="StepNumber3"
                                                    Width="20"
                                                    Height="20"
                                                    Background="#BDBDBD"
                                                    CornerRadius="50">
                                                    <TextBlock
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        FontFamily="{StaticResource SarabunRegular}"
                                                        FontSize="11"
                                                        Foreground="White"
                                                        Text="3" />
                                                </Border>
                                                <TextBlock
                                                    x:Name="StepText3"
                                                    Margin="10,0,0,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="{StaticResource SarabunRegular}"
                                                    FontSize="11"
                                                    FontWeight="SemiBold"
                                                    Foreground="#A7A7A7"
                                                    Text="SPK" />
                                            </StackPanel>
                                            <StackPanel Grid.Column="5" VerticalAlignment="Center">
                                                <Border
                                                    Width="15"
                                                    Height="1"
                                                    Background="#A7A7A7" />
                                            </StackPanel>
                                            <StackPanel
                                                Grid.Column="6"
                                                HorizontalAlignment="Left"
                                                Orientation="Horizontal">
                                                <Border
                                                    x:Name="StepNumber4"
                                                    Width="20"
                                                    Height="20"
                                                    Background="#BDBDBD"
                                                    CornerRadius="50">
                                                    <TextBlock
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        FontFamily="{StaticResource SarabunRegular}"
                                                        FontSize="11"
                                                        Foreground="White"
                                                        Text="4" />
                                                </Border>
                                                <TextBlock
                                                    x:Name="StepText4"
                                                    Margin="10,0,0,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="{StaticResource SarabunRegular}"
                                                    FontSize="11"
                                                    FontWeight="SemiBold"
                                                    Foreground="#A7A7A7"
                                                    Text="RAB" />
                                            </StackPanel>
                                            <StackPanel Grid.Column="7" VerticalAlignment="Center">
                                                <Border
                                                    Width="15"
                                                    Height="1"
                                                    Background="#A7A7A7" />
                                            </StackPanel>
                                            <StackPanel
                                                Grid.Column="8"
                                                HorizontalAlignment="Left"
                                                Orientation="Horizontal">
                                                <Border
                                                    x:Name="StepNumber5"
                                                    Width="20"
                                                    Height="20"
                                                    Background="#BDBDBD"
                                                    CornerRadius="50">
                                                    <TextBlock
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        FontFamily="{StaticResource SarabunRegular}"
                                                        FontSize="11"
                                                        Foreground="White"
                                                        Text="5" />
                                                </Border>
                                                <TextBlock
                                                    x:Name="StepText5"
                                                    Margin="10,0,0,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="{StaticResource SarabunRegular}"
                                                    FontSize="11"
                                                    FontWeight="SemiBold"
                                                    Foreground="#A7A7A7"
                                                    Text="Bayar RAB" />
                                            </StackPanel>
                                            <StackPanel Grid.Column="9" VerticalAlignment="Center">
                                                <Border
                                                    Width="15"
                                                    Height="1"
                                                    Background="#A7A7A7" />
                                            </StackPanel>
                                            <StackPanel
                                                Grid.Column="10"
                                                HorizontalAlignment="Left"
                                                Orientation="Horizontal">
                                                <Border
                                                    x:Name="StepNumber6"
                                                    Width="20"
                                                    Height="20"
                                                    Background="#BDBDBD"
                                                    CornerRadius="50">
                                                    <TextBlock
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        FontFamily="{StaticResource SarabunRegular}"
                                                        FontSize="11"
                                                        Foreground="White"
                                                        Text="6" />
                                                </Border>
                                                <TextBlock
                                                    x:Name="StepText6"
                                                    Margin="10,0,0,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="{StaticResource SarabunRegular}"
                                                    FontSize="11"
                                                    FontWeight="SemiBold"
                                                    Foreground="#A7A7A7"
                                                    Text="SPK/P" />
                                            </StackPanel>
                                            <StackPanel Grid.Column="11" VerticalAlignment="Center">
                                                <Border
                                                    Width="15"
                                                    Height="1"
                                                    Background="#A7A7A7" />
                                            </StackPanel>
                                            <StackPanel
                                                Grid.Column="12"
                                                HorizontalAlignment="Left"
                                                Orientation="Horizontal">
                                                <Border
                                                    x:Name="StepNumber7"
                                                    Width="20"
                                                    Height="20"
                                                    Background="#BDBDBD"
                                                    CornerRadius="50">
                                                    <TextBlock
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        FontFamily="{StaticResource SarabunRegular}"
                                                        FontSize="11"
                                                        Foreground="White"
                                                        Text="7" />
                                                </Border>
                                                <TextBlock
                                                    x:Name="StepText7"
                                                    Margin="10,0,0,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="{StaticResource SarabunRegular}"
                                                    FontSize="11"
                                                    FontWeight="SemiBold"
                                                    Foreground="#A7A7A7"
                                                    Text="BA" />
                                            </StackPanel>
                                            <StackPanel Grid.Column="13" VerticalAlignment="Center">
                                                <Border
                                                    Width="15"
                                                    Height="1"
                                                    Background="#A7A7A7" />
                                            </StackPanel>
                                            <StackPanel
                                                Grid.Column="14"
                                                HorizontalAlignment="Left"
                                                Orientation="Horizontal">
                                                <Border
                                                    x:Name="StepNumber8"
                                                    Width="20"
                                                    Height="20"
                                                    Background="#BDBDBD"
                                                    CornerRadius="50">
                                                    <TextBlock
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        FontFamily="{StaticResource SarabunRegular}"
                                                        FontSize="11"
                                                        Foreground="White"
                                                        Text="8" />
                                                </Border>
                                                <TextBlock
                                                    x:Name="StepText8"
                                                    Margin="10,0,5,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="{StaticResource SarabunRegular}"
                                                    FontSize="11"
                                                    FontWeight="SemiBold"
                                                    Foreground="#A7A7A7"
                                                    Text="Verifikasi" />
                                            </StackPanel>
                                        </Grid>
                                    </Grid>

                                    <TextBlock
                                        Foreground="{StaticResource SecondaryBlue}"
                                        Style="{StaticResource Title}"
                                        Text="Info Dasar Calon Pelanggan" />
                                    <Grid Margin="0,15">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="300" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="15" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="15" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="15" />
                                        </Grid.RowDefinitions>

                                        <StackPanel
                                            Grid.Row="0"
                                            Grid.Column="0"
                                            Focusable="{Binding IsEdit}"
                                            IsHitTestVisible="{Binding IsEdit}">
                                            <TextBlock Style="{StaticResource SmallTitle}" Text="Tipe Sambungan*" />
                                            <Grid Margin="0,0,15,0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*" />
                                                </Grid.ColumnDefinitions>
                                                <Grid Grid.Column="0">
                                                    <ComboBox
                                                        x:Name="TipePendaftaran"
                                                        Margin="0,4,0,0"
                                                        HorizontalAlignment="Stretch"
                                                        Background="White"
                                                        DisplayMemberPath="NamaTipePendaftaranSambungan"
                                                        ItemsSource="{Binding TipePendaftaranList}"
                                                        SelectedValue="{Binding TipePendaftaranForm, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                        SelectionChanged="KodePair_SelectionChanged"
                                                        Style="{StaticResource ComboBoxInputForm}"
                                                        Validation.ErrorTemplate="{x:Null}" />
                                                    <TextBlock
                                                        Padding="5,5,7,0"
                                                        DataContext="{Binding ElementName=TipePendaftaran, Path=SelectedItem}"
                                                        Foreground="{StaticResource Gray100}"
                                                        Style="{StaticResource ComboBoxSelectOverlay}"
                                                        Text="Pilih Tipe Sambungan" />
                                                </Grid>
                                            </Grid>
                                        </StackPanel>

                                        <StackPanel Grid.Row="2" Grid.Column="0">
                                            <TextBlock Style="{StaticResource SmallTitle}" Text="Nama*" />
                                            <Grid
                                                Margin="0,0,15,0"
                                                Focusable="{Binding IsEdit}"
                                                IsHitTestVisible="{Binding IsEdit}">
                                                <TextBox
                                                    x:Name="Nama"
                                                    PreviewKeyUp="CheckForm_PreviewKeyUp"
                                                    Style="{StaticResource TextBoxInputForm}"
                                                    Text="{Binding FormData.Nama, Mode=TwoWay}"
                                                    Validation.ErrorTemplate="{x:Null}" />
                                                <TextBlock
                                                    Padding="0"
                                                    DataContext="{Binding ElementName=Nama, Path=Text}"
                                                    Foreground="{StaticResource Gray100}"
                                                    Style="{StaticResource TextBoxPlaceholder}"
                                                    Text="Masukkan nama" />
                                            </Grid>
                                        </StackPanel>
                                        <StackPanel Grid.Row="2" Grid.Column="1">
                                            <TextBlock Style="{StaticResource SmallTitle}" Text="Tanggal Pendaftaran*" />
                                            <Grid
                                                Margin="0,0,15,0"
                                                Focusable="{Binding IsEdit}"
                                                IsHitTestVisible="{Binding IsEdit}">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="Auto" />
                                                </Grid.ColumnDefinitions>
                                                <DatePicker
                                                    x:Name="TglPermohonan"
                                                    Height="30"
                                                    HorizontalContentAlignment="Center"
                                                    BorderThickness="0.5"
                                                    SelectedDate="{Binding FormData.WaktuPermohonan}"
                                                    SelectedDateFormat="Long"
                                                    Style="{StaticResource DefaultDatePicker}" />
                                                <TextBlock
                                                    Grid.Column="0"
                                                    Padding="10,3,7,0"
                                                    Panel.ZIndex="-10"
                                                    DataContext="{Binding ElementName=TglPermohonan, Path=SelectedDate}"
                                                    Style="{StaticResource ComboBoxSelectOverlay}"
                                                    Text="Pilih tanggal" />
                                            </Grid>
                                        </StackPanel>
                                        <StackPanel Grid.Row="4" Grid.Column="0">
                                            <TextBlock Style="{StaticResource SmallTitle}" Text="Alamat*" />
                                            <Grid
                                                Margin="0,0,15,0"
                                                Focusable="{Binding IsEdit}"
                                                IsHitTestVisible="{Binding IsEdit}">
                                                <TextBox
                                                    x:Name="Alamat"
                                                    PreviewKeyUp="CheckForm_PreviewKeyUp"
                                                    Style="{StaticResource TextBoxInputForm}"
                                                    Text="{Binding FormData.Alamat, Mode=TwoWay}"
                                                    Validation.ErrorTemplate="{x:Null}" />
                                                <TextBlock
                                                    Padding="0"
                                                    DataContext="{Binding ElementName=Alamat, Path=Text}"
                                                    Foreground="{StaticResource Gray100}"
                                                    Style="{StaticResource TextBoxPlaceholder}"
                                                    Text="Masukkan alamat" />
                                            </Grid>
                                        </StackPanel>
                                        <Grid Grid.Row="4" Grid.Column="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition />
                                                <ColumnDefinition Width="10" />
                                                <ColumnDefinition />
                                            </Grid.ColumnDefinitions>
                                            <StackPanel Grid.Column="0">
                                                <TextBlock Style="{StaticResource SmallTitle}" Text="RT" />
                                                <Grid
                                                    Margin="0,0,15,0"
                                                    Focusable="{Binding IsEdit}"
                                                    IsHitTestVisible="{Binding IsEdit}">
                                                    <TextBox
                                                        x:Name="Rt"
                                                        PreviewKeyUp="CheckForm_PreviewKeyUp"
                                                        PreviewTextInput="NumberValidationTextBox"
                                                        Style="{StaticResource TextBoxInputForm}"
                                                        Text="{Binding FormData.Rt, Mode=TwoWay}"
                                                        Validation.ErrorTemplate="{x:Null}" />
                                                    <TextBlock
                                                        Padding="0"
                                                        DataContext="{Binding ElementName=Rt, Path=Text}"
                                                        Foreground="{StaticResource Gray100}"
                                                        Style="{StaticResource TextBoxPlaceholder}"
                                                        Text="000" />
                                                </Grid>
                                            </StackPanel>
                                            <StackPanel Grid.Column="2">
                                                <TextBlock Style="{StaticResource SmallTitle}" Text="RW" />
                                                <Grid
                                                    Margin="0,0,15,0"
                                                    Focusable="{Binding IsEdit}"
                                                    IsHitTestVisible="{Binding IsEdit}">
                                                    <TextBox
                                                        x:Name="Rw"
                                                        PreviewKeyUp="CheckForm_PreviewKeyUp"
                                                        PreviewTextInput="NumberValidationTextBox"
                                                        Style="{StaticResource TextBoxInputForm}"
                                                        Text="{Binding FormData.Rw, Mode=TwoWay}"
                                                        Validation.ErrorTemplate="{x:Null}" />
                                                    <TextBlock
                                                        Padding="0"
                                                        DataContext="{Binding ElementName=Rw, Path=Text}"
                                                        Foreground="{StaticResource Gray100}"
                                                        Style="{StaticResource TextBoxPlaceholder}"
                                                        Text="000" />
                                                </Grid>
                                            </StackPanel>
                                        </Grid>
                                    </Grid>
                                    <Grid Margin="0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="15" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="15" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="15" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="15" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="15" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <StackPanel Grid.Row="0" Grid.Column="0">
                                            <TextBlock Style="{StaticResource SmallTitle}" Text="No. Handphone*" />
                                            <Grid
                                                Margin="0,0,15,0"
                                                Focusable="{Binding IsEdit}"
                                                IsHitTestVisible="{Binding IsEdit}">
                                                <TextBox
                                                    x:Name="NoHandphone"
                                                    PreviewKeyUp="CheckForm_PreviewKeyUp"
                                                    PreviewTextInput="NumberValidationTextBox"
                                                    Style="{StaticResource TextBoxInputForm}"
                                                    Text="{Binding FormData.NoHp, Mode=TwoWay}"
                                                    Validation.ErrorTemplate="{x:Null}" />
                                                <TextBlock
                                                    Padding="0"
                                                    DataContext="{Binding ElementName=NoHandphone, Path=Text}"
                                                    Foreground="{StaticResource Gray100}"
                                                    Style="{StaticResource TextBoxPlaceholder}"
                                                    Text="Masukkan no handphone" />
                                            </Grid>
                                        </StackPanel>
                                        <StackPanel Grid.Row="0" Grid.Column="1">
                                            <TextBlock Style="{StaticResource SmallTitle}" Text="No. Telepon" />
                                            <Grid
                                                Margin="0,0,15,0"
                                                Focusable="{Binding IsEdit}"
                                                IsHitTestVisible="{Binding IsEdit}">
                                                <TextBox
                                                    x:Name="NoTelp"
                                                    PreviewKeyUp="CheckForm_PreviewKeyUp"
                                                    PreviewTextInput="NumberValidationTextBox"
                                                    Style="{StaticResource TextBoxInputForm}"
                                                    Text="{Binding FormData.NoTelp, Mode=TwoWay}"
